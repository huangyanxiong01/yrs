{
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "extends": "eslint:recommended",
  "rules": {
    "semi": 2,
    "no-console": 0,
    "no-cond-assign": 2,
    "no-constant-condition": 2,
    "no-unsafe-negation": 2,
    "no-template-curly-in-string": 2,
    "arrow-body-style": 2,
    "no-extra-parens": 2,
    "valid-jsdoc": 2,
    "accessor-pairs": 2,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "default-case": 1,
    "callback-return": 1,
    "no-sync": 2
  },
  "name": "yrs",
  "version": "1.0.0",
  "description": "Yarn registry switch tool",
  "main": "index.js",
  "repository": "git@github.com:huangyanxiong01/yrs.git",
  "author": "Freax <huangyanxiong2013@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.9.0"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-typeof-symbol": "^6.18.0",
    "babel-plugin-transform-inline-imports-commonjs": "^1.2.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-node4": "^2.1.0",
    "babel-preset-node5": "^11.1.0",
    "babel-preset-stage-0": "^6.16.0"
  }
}
